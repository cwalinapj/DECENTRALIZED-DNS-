# Resolver / Name Gateway
PORT=8054
UPSTREAM_DOH_URL=https://cloudflare-dns.com/dns-query
REQUEST_TIMEOUT_MS=5000
CACHE_TTL_MAX_S=3600
HOST=0.0.0.0
LOG_LEVEL=quiet

# Gated namespaces
GATED_SUFFIXES=.premium
VOUCHER_MODE=stub
VOUCHER_SECRET=

# Registry (.dns)
REGISTRY_ENABLED=0
REGISTRY_PATH=registry/snapshots/registry.json

# ENS
ENABLE_ENS=0
ETH_RPC_URL=
ENS_NETWORK=mainnet

# SNS
ENABLE_SNS=0
SOLANA_RPC_URL=https://api.devnet.solana.com
SNS_CLUSTER=devnet
